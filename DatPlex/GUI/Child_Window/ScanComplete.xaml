<UserControl x:Class="DatPlex.GUI.Child_Window.ScanComplete"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DatPlex.GUI.Child_Window"
             mc:Ignorable="d" 
             d:DesignHeight="125" d:DesignWidth="500">
    <Grid Background="#202020">
        <TextBlock Text="Scan Results:" Foreground="White" FontSize="18" TextDecorations="Underline" HorizontalAlignment="Center" Margin="0,0,0,80" 
                   TextWrapping="Wrap" VerticalAlignment="Center" Height="26" Width="300"/>
        <Label Name="mNewUsersLabel" Content="{Binding NewUsers_Lbl}" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,30" 
               Height="25" Width="300" Foreground="White"/>
        <Label Name="mNewItemsLabel" Content="{Binding NewItems_Lbl}" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,20,0,0" 
               Height="25" Width="300" Foreground="White"/>
        <Label Name="mRemItemsLabel" Content="{Binding RemItems_Lbl}" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,70,0,0"
               Height="25" Width="300" Foreground="White"/>
    </Grid>
</UserControl>
