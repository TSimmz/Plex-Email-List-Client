<UserControl x:Class="DatPlex.GUI.Child_Window.ScanInProgress"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DatPlex.GUI.Child_Window"
             mc:Ignorable="d" 
             d:DesignHeight="125" d:DesignWidth="500">
    <Grid Background="#202020">
        <Label Name="mScanLabel" Content="{Binding Progress_Lbl}" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,95,60" Height="25" Width="300"  
               Grid.Row="1" Foreground="White"/>
        <ProgressBar Name="mScanProgress" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,25" 
                     Height="25" Width="400" Grid.Row="1" IsIndeterminate="False"/>
    </Grid>
</UserControl>
